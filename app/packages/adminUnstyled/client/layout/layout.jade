template(name='orionCpLayout')
  // orionCpLayout
  .admin
    aside
      if dictionaryReady
        +reactiveTemplate template='links'
    main
      +orionCpHeader
      .admin-content
        if dictionaryReady
          +yield

template(name='orionCpOutAdminLayout')
  // orionCpOutAdminLayout
  .admin
    h1
      +yield 'title'
    +yield

template(name='orionCpHeader')
  // orionCpHeader
  header
    a.menu-toggle: i.fa.fa-bars

template(name='orionCpSmallContentOffset')
  // orionCpSmallContentOffset
  +yield

template(name='orionCpMediumContentOffset')
  // orionCpMediumContentOffset
  +yield

template(name='orionCpContentContainer')
  // orionCpContentContainer
  +yield

template(name='orionCpTitle')
  // orionCpTitle
  h1.title
    +yield
  hr

template(name='orionCpTabs')
  // orionCpTabs
  +each items
    a(class="{{ class }}")= title
